    "google-map-react": "^2.2.0",
    "got": "^11.7.0",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.3.18",
    "graphql-ws": "^5.12.0",
